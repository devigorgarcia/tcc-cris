// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int        @id @default(autoincrement())
  firstName  String
  lastName   String
  password   String
  enrollDate DateTime
  birthDate  DateTime
  isRh       Boolean    @default(false)
  state      String
  city       String
  holerite   Holerite[]
}

model Holerite {
  id           Int    @id @default(autoincrement())
  currentMonth String
  bruteSalary  Float
  inssValue    Float
  foodValue    Float
  healthyValue Float
  user         User   @relation(fields: [userId], references: [id])
  userId       Int
}
